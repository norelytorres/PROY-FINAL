function logear(){
	dataString = $("#login_header").serialize();
    document.getElementById("overlayer").setAttribute( 'style', 'z-index:1000000 !important' );
    $('#overlayer').css('display', 'block');
    $('#loading_unload_mc').css('display', 'block');
    $('#loading-bar-spinner').show();
	var valor = $.ajax({
	type: "POST",
	url: "/index.php?action=login_usuario_header&idioma=spa&id_tienda="+id_tienda_tradeinn,
	async: false,
	data: dataString,
	success: function(datos) {
        if (datos.includes("1"))
        {//valid
            document.getElementById("js_cookie_ajax").value=1;
            $('#mensaje_login_header').html("");
            document.getElementById("mensaje_login_header").style.display="none";
            //$('#login_header').submit();
            //console.log($('#template').val());
            if($('#template').val() == 'producto' || $('#template').val() == 'cesta') {
                window.location.reload();
            } else {
                window.location = "/index.php?action=mi_cuenta_new&idioma=spa&js_cookie_ajax=1&id_tienda="+id_tienda_tradeinn;
            }

        }
        else {
            $('#loading-bar-spinner').hide();
            document.getElementById('capa_login').style.display='block';
            document.getElementById("mensaje_login_header").style.display="block";
            document.getElementById("contrasena_superior").value="";
            $('#mensaje_login_header').html("El correo electrónico o contraseña no es válido");
           // $('#mensaje_login_header').html("Error en login");
            $('#overlayer').css('display', 'none');
            $('#loading_unload_mc').css('display', 'none');
            document.getElementById("overlayer").setAttribute( 'style', 'z-index:10000' );
            return false;
        }
	}
	}).fail( function() {
        document.getElementById('capa_login').style.display='block';
        document.getElementById("mensaje_login_header").style.display="block";
        document.getElementById("contrasena_superior").value="";
        $('#mensaje_login_header').html("El correo electrónico o contraseña no es válido");
        $('#overlayer').css('display', 'none');
        $('#loading_unload_mc').css('display', 'none');
        document.getElementById("overlayer").setAttribute( 'style', 'z-index:10000' );

        //$('#mensaje_login_header').html("Error en login");
        return false;

    });

    return false;
}
