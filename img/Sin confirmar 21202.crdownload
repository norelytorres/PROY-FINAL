				} else {
					$("#reemplazo_zend").css("display", "block");
				}
			} else {
				if (n <= 6 && (myhora > hora9 && myhora <= hora18)) {
					$('#xat').html('<script id="bim-snippet"  src="https://cwcentribot.centribal.com//app/chat?key=e912cb15787a4bdc8e18fc0559ea91dc.509eac9a6e0940c4a5a0d19e7adddedd" async="async" type="text/javascript"><\/script>');
				}
			}

			// si esta logeado muestro el whatsapp sino no lo muestro.
