	function habilitar_boton()
	{
		$('#boton_reportar_error_texto').show();
		if($("#tipo_de_error").val() != 0)
			$("#informe_error").prop("disabled", false);
		else
			$("#informe_error").prop("disabled", true);
	}

	function AddReadMore() {

		//This limit you can set after how much characters you want to show Read More.
		var mediaquery = window.matchMedia("(max-width : 480px )");
		if ( !mediaquery.matches) // si es mobile lo hago por css
			var carLmt = 3000;
		else
			var carLmt = 1200;

		// Text to show when text is collapsed
		var readMoreTxt = " ... Leer más";
		// Text to show when text is expanded
		var readLessTxt = " Ver menos";


		//Traverse all selectors with this class and manupulate HTML part to show Read More
		$(".addReadMore").each(function() {
			if ($(this).find(".firstSec").length)
				return;

			var allstr = $(this).html();
			if (allstr.length > carLmt) {
				var firstSet = allstr.substring(0, carLmt);
				lastchar = firstSet.substring(firstSet.length-1,firstSet.length);
				new_index = carLmt;
				while(lastchar != " " && new_index>0 ){
					new_index = new_index-1;
					firstSet = allstr.substring(0, new_index);
					lastchar = firstSet.substring(firstSet.length-1,firstSet.length);
				}
				var secdHalf = allstr.substring(new_index, allstr.length);
				var strtoadd = firstSet + "<span class='SecSec'>" + secdHalf + "</span><span class='readMore' title='"+readMoreTxt+"'>" + readMoreTxt + "</span><span class='readLess' title='"+readLessTxt+"'>" + readLessTxt + "</span>";
				$(this).html(strtoadd);
			}

		});
		//Read More and Read Less Click Event binding
		$(document).on("click", ".readMore,.readLess", function() {
			if(!read_more) {
				$('.SecSec').show();
				$('.readLess').show();
				$('.readMore').hide();
				read_more = true;
			} else {
				$('.SecSec').hide();
				read_more = false;
				$('.readLess').hide();
				$('.readMore').show();
			}
		});
	}
	$(function() {
		//Calling function after Page Load
		AddReadMore();
	});



</script>

