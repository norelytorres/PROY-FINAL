            var meumobile = window.matchMedia("(max-width : 480px )");
            if ( meumobile.matches )
            {
                $('.zona_logobuscador').addClass('fixed');
                $('.content').css('margin-top', (altura_del_menu) + 'px');
            }

        } else {
            $('.zona_logobuscador').removeClass('fixed');
            $('.content').css('margin-top', '0');
        }
    }


